{
  "spider": "universal",
  "website": "越南政府网中文版",
  "describe": "中文版",
  "index": "http://cn.news.chinhphu.vn/",
  "settings": {
    "USER_AGENT": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_12_6) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/60.0.3112.90 Safari/537.36"
  },
  "start_urls": [
    "http://cn.news.chinhphu.vn/"
  ],
  "allowed_domains": [
    "chinhphu.vn"
  ],
  "rules": "chinhphu-cn",
  "item": {
    "class": "ChinhphuItem",
    "loader": "ChinhphuLoader",
    "datetime_format": "%H:%M | %Y/%m/%d",
    "attrs": {
      "news_title": [
        {
          "method": "xpath",
          "args": [
            "//div[@class=\"dtContentHl\"]/span[@id=\"ctl00_mainContent_bodyContent_lbHeadline\"]/text()"
          ]
        }
      ],
      "news_content": [
        {
          "method": "xpath",
          "args": [
            "//div[@class=\"dtContentTxt\"]//text()"
          ]
        }
      ],
      "public_date": [
        {
          "method": "xpath",
          "args": [
            "//div[@class=\"dtContentDate\"]/span[@id=\"ctl00_mainContent_bodyContent_lbDate\"]/text()"
          ]
        }
      ],
      "site": [
        {
          "method": "value",
          "args": [
            "http://cn.news.chinhphu.vn/"
          ]
        }
      ],
      "author": [
        {
          "method": "value",
          "args": [
            "anonymous"
          ]
        }
      ],
      "media": [
        {
          "method": "value",
          "args": [
            "chinhphu-cn"
          ]
        }
      ],
      "type": [
        {
          "method": "value",
          "args": [
            "None"
          ]
        }
      ],
      "abstract": [
        {
          "method": "value",
          "args": [
            "None"
          ]
        }
      ],
      "url": [
        {
          "method": "attr",
          "args": [
            "url"
          ]
        }
      ]
    }
  }
}